{
  "hosting": {
    "public": "Y",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "clipboard-polyfill.js",
        "destination": "/clipboard-polyfill.js"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
